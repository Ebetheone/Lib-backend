{
  "name": "lib-backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && tsc && generate:prisma && generate",
    "clean": "rm -rf dist",
    "merge:prisma": "node prisma/index.js && yarn prisma format && yarn prisma validate",
    "generate:prisma": "yarn merge:prisma && yarn prisma generate",
    "generate:nexus": "ts-node-dev --transpile-only --exit-child -r tsconfig-paths/register src/lib/schema",
    "generate:codegen": "graphql-codegen --config codegen.ts",
    "generate": "yarn generate:prisma && yarn generate:nexus && yarn generate:codegen",
    "dev": "ts-node-dev --respawn --transpile-only --exit-child --clear -r tsconfig-paths/register src/index.ts",
    "start": "yarn build && ts-node --transpile-only -r tsconfig-paths/register dist/index.js",
    "lint": "tsc --noEmit && eslint . --ext .js,.jsx,.ts,.tsx",
    "prisma:migrate": "yarn generate:prisma && yarn prisma migrate dev --name init"
  },
  "dependencies": {
    "@apollo/server": "^4.7.1",
    "@casl/ability": "^6.5.0",
    "@faker-js/faker": "^8.0.1",
    "@google-cloud/storage": "^6.10.1",
    "@prisma/client": "^5.1.1",
    "@types/node-fetch": "^2.6.4",
    "@types/nodemailer": "^6.4.8",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "clear": "^0.1.0",
    "cors": "^2.8.5",
    "cron": "^2.3.0",
    "csv": "^6.3.0",
    "csv-parse": "^5.3.10",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.0.3",
    "firebase": "^9.22.0",
    "firebase-admin": "^11.8.0",
    "graphql": "^16.6.0",
    "graphql-middleware": "^6.1.34",
    "graphql-scalars": "^1.22.0",
    "graphql-shield": "^7.6.5",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.13.1",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "mongodb": "^6.3.0",
    "nexus": "^1.3.0",
    "node-device-detector": "^2.0.16",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.2",
    "winston": "^3.8.2",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@casl/prisma": "^1.4.0",
    "@graphql-codegen/cli": "^3.3.1",
    "@graphql-codegen/introspection": "^3.0.1",
    "@graphql-codegen/typescript": "^3.0.4",
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.194",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^20.2.3",
    "@types/uuid": "^9.0.1",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "bcryptjs": "^2.4.3",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-mock-extended": "^3.0.4",
    "nexus-prisma": "^2.0.0",
    "prettier": "^2.8.8",
    "prisma": "^5.1.1",
    "prisma-dbml-generator": "^0.10.0",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "engines": {
    "node": "18.x",
    "yarn": "1.22.x"
  }
}
